{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "i18n-ally.localesPaths": [
        "assets/flutter_i18n"
    ],
    "i18n-ally.sourceLanguage": "en",
    "cSpell.words": [
        "fixnum",
        "fluttertoast",
        "Fluttertoast",
        "protobuf",
        "rumgap",
        "wuxia"
    ],
    "dart.lineLength": 130,
    "i18n-ally.keystyle": "nested",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "java.configuration.updateBuildConfiguration": "interactive",
}